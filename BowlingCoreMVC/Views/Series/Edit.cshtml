@model BowlingCoreMVC.Models.Series

@{
    ViewData["Title"] = "Edit";
}

@{
    var m = Model;
}
<h2>Edit</h2>


<div class="row">
    <div class="col-lg-6">
        <input type="button" class="btn btn-success btn-block" value="Save All" onclick="SaveAllClick()" />
    </div>
</div>

@foreach (var g in Model.Games)
{
    @await Component.InvokeAsync("EditGame", new { GameID = g.ID })

}

